/dts-v1/;

/ {
      compatible = "Synology";
      model = "synology_geminilake_920+";
	
      internal_slot@1 {
		protocol_type = "sata";
		power_pin_gpio = <0x14 0x0>;
		detect_pin_gpio = <0x23 0x1>;
		led_type = "lp3943";

		ahci {
			pcie_root = "00:1c.0,00.0";
			ata_port = <0x0>;
		};

		led_green {
			led_name = "syno_led0";
		};

		led_orange {
			led_name = "syno_led1";
		};
	};
	internal_slot@2 {
		protocol_type = "sata";
		power_pin_gpio = <0x15 0x0>;
		detect_pin_gpio = <0x24 0x1>;
		led_type = "lp3943";

		ahci {
			pcie_root = "00:1c.0,00.0";
			ata_port = <0x1>;
		};

		led_green {
			led_name = "syno_led2";
		};

		led_orange {
			led_name = "syno_led3";
		};
	};
        internal_slot@3 {
		protocol_type = "sata";
		power_pin_gpio = <0x16 0x0>;
		detect_pin_gpio = <0x25 0x1>;
		led_type = "lp3943";

		ahci {
			pcie_root = "00:1c.1,00.0";
			ata_port = <0x2>;
		};

		led_green {
			led_name = "syno_led4";
		};

		led_orange {
			led_name = "syno_led5";
		};
	};
	internal_slot@4 {
		protocol_type = "sata";
		power_pin_gpio = <0x17 0x0>;
		detect_pin_gpio = <0x26 0x1>;
		led_type = "lp3943";

		ahci {
			pcie_root = "00:1c.1,00.0";
			ata_port = <0x3>;
		};

		led_green {
			led_name = "syno_led6";
		};

		led_orange {
			led_name = "syno_led7";
		};
	};
	internal_slot@5 {
		protocol_type = "sata";
		power_pin_gpio = <0x18 0x0>;
		detect_pin_gpio = <0x27 0x1>;
		led_type = "lp3943";

		ahci {
			pcie_root = "00:1c.1,00.0";
			ata_port = <0x4>;
		};

		led_green {
			led_name = "syno_led8";
		};

		led_orange {
			led_name = "syno_led9";
		};
	};
	internal_slot@6 {
		protocol_type = "sata";
		power_pin_gpio = <0x19 0x0>;
		detect_pin_gpio = <0x28 0x1>;
		led_type = "lp3943";

		ahci {
			pcie_root = "00:1c.1,00.0";
			ata_port = <0x5>;
		};

		led_green {
			led_name = "syno_led10";
		};

		led_orange {
			led_name = "syno_led11";
		};
	};

	nvme_slot@1 {
		pcie_root = "00:1c.2";
		port_type = "ssdcache";
	};

	nvme_slot@2 {
		pcie_root = "00:1c.3";
		port_type = "ssdcache";
	};
};
